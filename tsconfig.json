{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "__e2e__",
    "__tests__",
    "src",
    "env.d.ts"
  ],
  "exclude": [
    "public",
    "dist"
  ],
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "$/*": [
        "./*"
      ]
    },
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}