---
import type { HTMLAttributes } from 'astro/types';

interface Props extends HTMLAttributes<'svg'> {
}

const { type, class: classname, ...props } = Astro.props;
---

<svg
  xmlns='http://www.w3.org/2000/svg'
  fill='none'
  viewBox='0 0 24 24'
  stroke-width='1.5'
  stroke='currentColor'
  class={classname}
  {...props}
>
  <slot></slot>
</svg>
